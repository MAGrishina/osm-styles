name: Default Styler
feature-styles:
- name: name
  rules:
  - filter: ${place = 'large_town'}
    scale: ['100000.0', '200000.0']
    symbolizers:
    - text:
        label: ${name}
        fill-color: '#555555'
        halo:
          fill-color: '#FFFFFF'
          radius: 2
        fon-family: DejaVu Sans, Unifont Medium, Unifont Medium
        font-size: 12.0
        font-style: normal
        font-weight: normal
        placement: point
        x-group: true
